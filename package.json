{
  "name": "yandex_practicum",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run build && node index.ts",
    "check": "tsc --noEmit",
    "test": "mocha --require ignore-styles",
    "build": "parcel build static/index.html",
    "dev": "parcel  static/index.html --port=3000 --open",
    "lint": "eslint --fix ./ --ext .ts",
    "stylelint": "stylelint  \"src/**/*.{css,less}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,less}\" --fix",
    "prettier": "prettier --write ./ --ext .ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix ./ --ext .ts"
    ],
    "*.(css|less)": [
      "stylelint  \"src/**/*.{css,less}\" --fix"
    ]
  },
  "author": "drozdovdn",
  "license": "ISC",
  "dependencies": {
    "@parcel/optimizer-data-url": "^2.5.0",
    "@parcel/resolver-default": "^2.5.0",
    "@types/express": "^4.17.13",
    "@types/websocket": "^1.0.5",
    "express": "^4.17.2",
    "nanoid": "^3.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/register": "^7.17.7",
    "@parcel/config-default": "^2.2.0",
    "@parcel/transformer-less": "^2.2.0",
    "@parcel/transformer-typescript-tsc": "^2.6.0",
    "@parcel/validator-typescript": "^2.6.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.38",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "assert": "^2.0.0",
    "autoprefixer": "^10.4.1",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "chai": "^4.3.6",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "events": "^3.3.0",
    "husky": "^7.0.4",
    "ignore-styles": "^5.0.1",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.2.1",
    "mocha": "^10.0.0",
    "parcel": "^2.5.0",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.5",
    "postcss-custom-properties": "^12.0.4",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-url": "^10.1.3",
    "prettier": "2.5.1",
    "querystring-es3": "^0.2.1",
    "sinon": "^14.0.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "stylelint": "^13.13.1",
    "stylelint-config-airbnb": "0.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-less": "^1.0.1",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.5.4",
    "url": "^0.11.0"
  },
  "staticFiles": {
    "staticPath": "./static/icons",
    "watcherGlob": "**"
  },
  "engines": {
    "node": "14.19.3",
    "parsel": "2.5.0"
  },
  "targets": {
    "modern": {
      "engines": {
        "browsers": "Chrome 80"
      }
    },
    "legacy": {
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  }
}
